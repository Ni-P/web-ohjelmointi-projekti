
<% include ../partials/header.ejs %>

<form class="col s12 form-content" method="POST">
    <div class="row">
        <div class="input-field col s12">
            <label for="username">Username</label>
            <input id="username-field" type="text" name="username" class="validate">
        </div>
    </div>
    <div class="row">
        <div class="input-field col s12">
            <label for="password-field">Password</label>
            <input id="password-field" type="password" name="password" class="validate">
        </div>
    </div>
    <div class="row">
        <div class="col s12">
            <input class="btn green darken-3" type="submit"/>
            <a class="btn red darken-1" href="/">Cancel</a>
        </div>
    </div>
</form>

<% include ../partials/footer.ejs %>